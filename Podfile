project 'bottle.xcodeproj'

# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'

use_frameworks!
inhibit_all_warnings!

def sharePods
  pod 'Alamofire', '5.4.4' # 网络库
  pod 'SwiftyJSON' # JSON解析库
  pod 'HandyJSON' # JSON解析库
  pod 'SwifterSwift' # swift工具库
  pod 'WCDB.swift' # WCDB数据库
end

target 'bottle' do
  
  sharePods
  pod 'SnapKit' # 布局约束库
  pod 'IQKeyboardManagerSwift' # 键盘管理库
  pod 'SDWebImage' # 图片加载库
  pod 'SKPhotoBrowser', :git => 'https://github.com/suzuki-0000/SKPhotoBrowser.git', :branch => 'master' # 图片预览库
  pod 'ZLPhotoBrowser' # 图片管理库
  pod 'CryptoSwift' # 加密库
  pod 'DZNEmptyDataSet' # 空数据库
  pod 'LXFProtocolTool/LXFEmptyDataSetable', :modular_headers => true # 空数据协议
  pod 'lottie-ios' # 动画库
  pod 'BRPickerView' # 选择器库
  pod 'ESTabBarController-swift' # tabBar管理库
  # Pods for bottle
  
end
